<!DOCTYPE html>
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    
    title TicTacToe Game
    
    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css" integrity="sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ" crossorigin="anonymous")
    script(src="https://use.fontawesome.com/6af8722801.js")
    link(rel="stylesheet", href="main.css")
  body
    .container
      h1.text-center Tic Tac Toe
      main.grid-container
        .row.a.text-center
          .col.empty.used(data-row="0" data-col="a")
          .col.empty(data-row="0" data-col="b")
          .col.empty(data-row="0" data-col="c")
        .row.b.text-center
          .col.empty(data-row="1" data-col="a")
          .col.empty(data-row="1" data-col="b")
          .col.empty(data-row="1" data-col="c")
        .row.c.text-center
          .col.empty(data-row="2" data-col="a")
          .col.empty(data-row="2" data-col="b")
          .col.empty(data-row="2" data-col="c")
    
    .pop-up-window#side
      .wrapper
        h2.msg-txt.text-uppercase.text-center Choose your side
        .btn-container
          .btn-wrapper.btn-left-wrapper
            .btn-txt.btn-left-txt.text-uppercase X
          .btn-wrapper.btn-right-wrapper
            .btn-txt.btn-right-txt.text-uppercase O
              

  script(src="https://code.jquery.com/jquery-3.1.1.slim.min.js" integrity="sha384-A7FZj7v+d/sdmMqp/nOQwliLvUsJfDHW+k9Omg/a/EheAdgtzNs3hpfag6Ed950n" crossorigin="anonymous")
  script(src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous")
  script(src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js" integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn" crossorigin="anonymous")
  script(src="main.js")
